DDOC =
$(DEFINE $(TITLE)_BLURB,
  $(T a, class="name" href="$(SPEAKER_URL)", $(T h1,, $(SPEAKER)))
  $(DIV class="row",
    $(DIV class="rowitem",
      <img src="$(SPEAKER_PIC)" height="170" alt="$(SPEAKER)">
    )
    $(DIV class="rowitem",
      $(BIO)
    )
  )
)


SPEAKER_PIC = $(BASE)/../2018/images/stock.png

BASE = ..
